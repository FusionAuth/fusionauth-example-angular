{
    "variables": {
        "applicationId": "E9FDB985-9173-4E01-9D73-AC2D60D1DC8E",
        "apiKey": "this_really_should_be_a_long_random_alphanumeric_value_but_this_still_works",
        "asymmetricKeyId": "#{UUID()}",
        "defaultTenantId": "d7d09513-a3f5-401c-9685-34ab6c552453",
        "adminEmail": "admin@fusionauthexample.com",
        "adminPassword": "adminpassword",
        "adminUserId": "00000000-0000-0000-0000-000000000001",
        "userEmail": "user@fusionauthexample.com",
        "userPassword": "userpassword",
        "userUserId":  "00000000-0000-0000-0000-111111111111"
    },
    "apiKeys": [
        {
            "key": "#{apiKey}",
            "description": "Unrestricted API key"
        }
    ],
    "requests": [
        {
            "method": "POST",
            "url": "/api/key/generate/#{asymmetricKeyId}",
            "tenantId": "#{defaultTenantId}",
            "body": {
              "key": {
                "algorithm": "RS256",
                "name": "For exampleapp",
                "length": 2048
              }
            }
        },
        {
            "method": "POST",
            "url": "/api/application/#{applicationId}",
            "tenantId": "#{defaultTenantId}",
            "body": {
              "application": {
                "name": "exampleapp",
                "oauthConfiguration" : {
                    "authorizedRedirectURLs": ["http://localhost:5000/signin-oidc"],
                    "logoutURL": "http://localhost:5000",
                    "enabledGrants": ["authorization_code"]
                },
                "jwtConfiguration": {
                  "enabled": true,
                  "accessTokenKeyId": "#{asymmetricKeyId}",
                  "idTokenKeyId": "#{asymmetricKeyId}"
                }
              }
            }
        },
        {
            "method": "POST",
            "url": "/api/user/registration/#{adminUserId}",
            "body": {
                "user": {
                    "birthDate": "1981-06-04",
                    "email": "#{adminEmail}",
                    "firstName": "Dinesh",
                    "lastName": "Chugtai",
                    "password": "#{adminPassword}"
                },
                "registration": {
                    "applicationId": "#{applicationId}"
                }
            }
        },
        {
            "method": "POST",
            "url": "/api/user/registration/#{userUserId}",
            "body": {
                "user": {
                    "birthDate": "1985-11-23",
                    "email": "#{userEmail}",
                    "firstName": "Fred",
                    "lastName": "Flintstone",
                    "password": "#{userPassword}"
                },
                "registration": {
                    "applicationId": "#{applicationId}"
                }
            }
        }
    ]
}
